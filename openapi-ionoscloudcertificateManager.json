            }
          }
        },
        "CertificatePropertiesDto": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "example": "My Certificate",
              "description": "The certificate name."
            },
            "certificate": {
              "type": "string",
              "example": "-----BEGIN CERTIFICATE-----MIIE5TCCAs2gAwIBAgIBATANBgkqhkiG9w0BAQsFADA2MQswCQYDVQQGEwJSTzEK-----END CERTIFICATE-----",
              "description": "The certificate body."
            },
            "certificateChain": {
              "type": "string",
              "example": "-----BEGIN CERTIFICATE-----MIIDoTCCAokCFDrAUWffdxWJVz2Axl9lp/4xiUteMA0GCSqGSIb3DQEBCwUAMIGG-----END CERTIFICATE-----",
              "description": "Optional. The certificate chain."
            }
          }
        },
        "CertificatePostPropertiesDto": {
          "x-java-class-annotation": "@PemFormatMatch",
          "type": "object",
          "required": [
            "name",
            "certificate",
            "certificateChain",
            "privateKey"
          ],
          "properties": {
            "name": {
              "type": "string",
              "example": "My Certificate",
              "description": "The certificate name."
            },
            "certificate": {
              "type": "string",
              "example": "-----BEGIN CERTIFICATE-----MIIE5TCCAs2gAwIBAgIBATANBgkqhkiG9w0BAQsFADA2MQswCQYDVQQGEwJSTzEK-----END CERTIFICATE-----",
              "description": "The certificate body."
            },
            "certificateChain": {
              "type": "string",
              "example": "-----BEGIN CERTIFICATE-----MIIDoTCCAokCFDrAUWffdxWJVz2Axl9lp/4xiUteMA0GCSqGSIb3DQEBCwUAMIGG-----END CERTIFICATE-----",
              "description": "The certificate chain."
            },
            "privateKey": {
              "type": "string",
              "example": "-----BEGIN RSA PRIVATE KEY-----MIIJKQIBAAKCAgEAzDehfqWBr+9q0pxwCDDRph7QSPiMbkDGaGKc+Fd2h3doT8Li-----END RSA PRIVATE KEY-----",
              "description": "The RSA private key is used for authentication and symmetric key exchange when establishing an SSL session. It is a part of the public key infrastructure generally used with SSL certificates."
            }
          }
        },
        "CertificatePatchPropertiesDto": {
          "type": "object",
          "required": [
            "name"
          ],
          "properties": {
            "name": {
              "type": "string",
              "example": "My Certificate",
              "description": "The certificate name."
            }
          }
        },
        "ConfigPropertyDto": {
          "type": "object",
          "required": [
            "name",
            "value",
            "description"
          ],
          "properties": {
            "id": {
              "type": "string",
              "description": "The property id.",
              "readOnly": true
            },
            "name": {
              "type": "string",
              "description": "The property name."
            },
            "value": {
              "type": "string",
              "description": "The property value."
            },
            "description": {
              "type": "string",
              "description": "The property description."
            }
          }
        }
      }
    }
