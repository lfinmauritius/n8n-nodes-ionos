components:
  parameters:
    paginationOffset:
      name: offset
      in: query
      description: The first element (of the total list of elements) to include in the response. Use together with limit for pagination.
      required: false
      schema:
        type: integer
        format: int32
        default: 0
        minimum: 0
      example: 0
    paginationLimit:
      name: limit
      in: query
      description: The maximum number of elements to return. Use together with offset for pagination.
      required: false
      schema:
        type: integer
        format: int32
        default: 100
        minimum: 1
        maximum: 1000
      example: 100
  schemas:
    Connection:
      description: Details about the network connection for your VPN Gateway.
      required:
        - datacenterId
        - lanId
        - ipv4CIDR
      type: object
      properties:
        datacenterId:
          description: |
            The datacenter to connect your VPN Gateway to.
          type: string
          example: '5a029f4a-72e5-11ec-90d6-0242ac120003'
        lanId:
          description: The numeric LAN ID to connect your VPN Gateway to.
          type: string
          example: '2'
        ipv4CIDR:
          description: |
            A LAN IPv4 address in CIDR notation that will be assigned to the VPN Gateway.
            This will be the private gateway address for LAN clients to route traffic over the VPN Gateway.
            __Note__: this should be within the subnet already assigned to the LAN
          allOf:
            - $ref: '#/components/schemas/IPV4CIDR'
        ipv6CIDR:
          description: |
            A LAN IPv6 address in CIDR notation that will be assigned to the VPN Gateway.
            This will be the private gateway address for LAN clients to route traffic over the VPN Gateway.
            __Note__: this should be within the subnet already assigned to the LAN
          allOf:
            - $ref: '#/components/schemas/IPV6CIDR'
  
    CIDR:
      type: string
      anyOf:
        - $ref: '#/components/schemas/IPV4CIDR'
        - $ref: '#/components/schemas/IPV6CIDR'
    IPV6CIDR:
      type: string
      pattern: "^(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:))\\/([0-9]|[0-9][0-9]|1[0-1][0-9]|12[0-8])$"
      example: fd28:5f8d:d9fe:08bd::/64
      description: |
        Describes a range of IP V6 addresses in CIDR notation.
    IPV4CIDR:
      type: string
      pattern: "^((([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]))\\/([0-9]|[1-2][0-9]|3[0-2])$"
      example: 192.168.1.100/24
      description: |
        Describes a range of IP V4 addresses in CIDR notation.
    GatewayIP:
      type: string
      anyOf:
        - format: ipv4
      example: 81.173.1.2
      description: |
        Public IP address to be assigned to the gateway.
        __Note__: This must be an IP address in the same datacenter as the connections.
    IPPort:
      type: integer
      description: IP port number
      example: 51820
      minimum: 0
      maximum: 65535
    DiffieHellmanGroup:
      type: string
      description: |
        The Diffie-Hellman Group to use for IPSec Encryption.\
        Options:
          - 15-MODP3072
          - 16-MODP4096
          - 19-ECP256
          - 20-ECP384
          - 21-ECP521
          - 28-ECP256BP
          - 29-ECP384BP
          - 30-ECP512BP
      x-extensible-enum:
      - 15-MODP3072
      - 16-MODP4096
      - 19-ECP256
      - 20-ECP384
      - 21-ECP521
      - 28-ECP256BP
      - 29-ECP384BP
      - 30-ECP512BP
      default: 16-MODP4096
    EncryptionAlgorithm:
      type: string
      description: |
        The encryption algorithm to use for IPSec Encryption.\
        Options:
        - AES128-CTR
        - AES256-CTR
        - AES128-GCM-16
        - AES256-GCM-16
        - AES128-GCM-12
        - AES256-GCM-12
        - AES128-CCM-12
        - AES256-CCM-12
        - AES128
        - AES256
      x-extensible-enum:
      - AES128-CTR
      - AES256-CTR
      - AES128-GCM-16
      - AES256-GCM-16
      - AES128-GCM-12
      - AES256-GCM-12
      - AES128-CCM-12
      - AES256-CCM-12
      - AES128
      - AES256
      default: AES256
    IntegrityAlgorithm:
      type: string
      description: |
        The integrity algorithm to use for IPSec Encryption.\
        Options:
        - SHA256
        - SHA384
        - SHA512
        - AES-XCBC
      x-extensible-enum:
      - SHA256
      - SHA384
      - SHA512
      - AES-XCBC
      default: SHA256
    IKEEncryption:
      type: object
      description: Settings for the initial security exchange phase.
      properties:
        diffieHellmanGroup:
          allOf:
            - $ref: '#/components/schemas/DiffieHellmanGroup'
        encryptionAlgorithm:
          allOf:
            - $ref: '#/components/schemas/EncryptionAlgorithm'
        integrityAlgorithm:
          allOf:
            - $ref: '#/components/schemas/IntegrityAlgorithm'
        lifetime:
          type: integer
          description: The phase lifetime in seconds.
          default: 86400
          example: 86400
          minimum: 3600
          maximum: 86400
    ESPEncryption:
      type: object
      description: Settings for the IPSec SA (ESP) phase.
      properties:
        diffieHellmanGroup:
          allOf:
            - $ref: '#/components/schemas/DiffieHellmanGroup'
        encryptionAlgorithm:
          allOf:
            - $ref: '#/components/schemas/EncryptionAlgorithm'
        integrityAlgorithm:
          allOf:
            - $ref: '#/components/schemas/IntegrityAlgorithm'
        lifetime:
          type: integer
          description: The phase lifetime in seconds.
          default: 3600
          example: 3600
          minimum: 600
          maximum: 14400
    Tier:
      type: string
      description: |
        Gateway performance options.  See product documentation for full details.\
        Options:
        - STANDARD
        - STANDARD_HA
        - ENHANCED
        - ENHANCED_HA
        - PREMIUM
        - PREMIUM_HA
      x-extensible-enum:
        - STANDARD
        - STANTARD_HA
        - ENHANCED
        - ENHANCED_HA
        - PREMIUM
        - PREMIUM_HA
      default: STANDARD
