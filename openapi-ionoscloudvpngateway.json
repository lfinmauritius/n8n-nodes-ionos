      security:
      - tokenAuth: []
      tags:
        - WireguardGateways
      description: Deletes the specified WireguardGateway.
      parameters:
        - name: gatewayId
          in: path
          schema:
            type: string
            format: uuid
          example: "85c79b4b-5b40-570a-b788-58dd46ea71e2"
          required: true
          description: The ID (UUID) of the WireguardGateway.
      responses:
        "202":
          description: Deleting WireguardGateway was successful.
        "400":
          $ref: '#/components/responses/BadRequest'
